# RAG QnA System Environment Variables

# Qdrant Local Configuration
QDRANT_HOST=localhost
QDRANT_PORT=6333
QDRANT_URL=http://localhost:6333
QDRANT_COLLECTION_NAME=pdf_embeddings

# Embedding Model Configuration
EMBEDDING_MODEL=all-MiniLM-L6-v2
EMBEDDING_DIMENSION=384
BATCH_SIZE=32

# Gemini API Configuration
GEMINI_API_KEY=your_gemini_api_key_here

# Web Interface Configuration
FLASK_SECRET_KEY=your-secret-key-change-this-in-production
FLASK_HOST=127.0.0.1
FLASK_PORT=5000

# Processing Configuration
CHUNK_SIZE=1000
OVERLAP_SIZE=200
MAX_TOKENS=8192

# File Paths
JSON_DATA_DIR=./extraceted PDF data
EMBEDDINGS_OUTPUT_DIR=./embeddings_output
PROCESSED_DATA_DIR=./processed_data

# Logging Configuration
LOG_LEVEL=INFO
LOG_FILE=rag_qna_system.log

# Performance Settings
MAX_WORKERS=4
TIMEOUT=30
RETRY_ATTEMPTS=3

# Search Configuration
TOP_K_RESULTS=5
SIMILARITY_THRESHOLD=0.7

# Language Settings
DEFAULT_LANGUAGE=en
SUPPORTED_LANGUAGES=en,mr

# Cache Settings
CACHE_ENABLED=true
CACHE_DIR=./cache
CACHE_TTL=3600
